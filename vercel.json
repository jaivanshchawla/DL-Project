{
  "buildCommand": "npm install --prefix frontend && npm --prefix frontend run build",
  "outputDirectory": "frontend/build",
  "devCommand": "npm --prefix frontend start",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
